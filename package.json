{
	"name": "csp-migration",
	"version": "0.0.0",
	"description": "A blank schematics",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"test": "npm run build && jasmine src/**/*_spec.js",
		"watch": "tsc -p tsconfig.json --watch",
		"prepare": "husky install"
	},
	"keywords": [
		"schematics"
	],
	"author": "Nikhil, Hukkerikar <nhukkerikar@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nikhil618/csp-migration.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
		"**/*.{js,jsx,ts,tsx}": [
			"npx prettier --write --ignore-unknown",
			"npx eslint --fix"
		]
	},
	"bugs": {
		"url": "https://github.com/nikhil618/csp-migration/issues"
	},
	"homepage": "https://github.com/nikhil618/csp-migration#readme",
	"schematics": "./src/collection.json",
	"dependencies": {
		"@angular-devkit/core": "^15.1.1",
		"@angular-devkit/schematics": "^15.1.1",
		"@schematics/angular": "^15.1.2"
	},
	"devDependencies": {
		"@types/jasmine": "~4.3.0",
		"@types/node": "^14.15.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-config-standard-with-typescript": "^31.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.3",
		"jasmine": "^4.0.0",
		"lint-staged": "^13.1.0",
		"prettier": "2.8.3",
		"typescript": "^4.9.4"
	},
	"ng-add": {
		"save": "dependencies"
	}
}
